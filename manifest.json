{
    "manifest_version": 2,
    "name": "Baidu Mp3",
    "description": "This extension helps to extract baidu music audio",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Mp3 Extractor"
    },

    "background": {
        "scripts": ["main.js"]
    },

    "permissions" :[
        "downloads",
        "webRequest",
        "webRequestBlocking",
        "notifications",
        "*://*.baidu.com/"
    ]
}
